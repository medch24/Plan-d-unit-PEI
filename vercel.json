{
  "version": 2,
  "builds": [
    { "src": "api/index.js", "use": "@vercel/node" }
  ],
  "routes": [
    { "src": "/api/(.*)", "dest": "api/index.js" },
    { "src": "^/$", "dest": "/public/index.html" },
    { "src": "/favicon\\.(ico|png|svg)", "dest": "/public/favicon.svg" },
    { "src": "/(.*\\.(css|js|html|svg|docx))", "dest": "/public/$1" },
    { "src": "/(.*)", "dest": "/public/index.html" }
  ]
}
